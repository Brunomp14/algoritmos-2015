#include "stdafx.h"
#include "stdio.h"
#include "conio.h"
#include "math.h"
#include "iostream"
using namespace std;

void main (void)
{
	float a, b, c, d, e, f, x, y;
	cout <<"Ingrese a, b, c";
	cin >> a >> b >> c;
	cout <<"Ingrese d, e, f";
	cin >> d >> e >> f;
	y = (a*f - d*c)/(a*e - b*d);
	x = (c - b*y)/a;
	cout <<"X = " << x;
	cout <<"Y = " << y;
	getch ();
}
